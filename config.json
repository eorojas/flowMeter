{
  "sensors": {
    "flow": {
      "frequency_hz": 100,
      "resolution_bits": 24
    },
    "pressure": {
      "frequency_hz": 10,
      "resolution_bits": 8
    },
    "temperature": {
      "frequency_hz": 10,
      "resolution_bits": 8
    }
  },
  "flow_equation": "flow * (pressure / 255.0) * (temperature / 255.0)"
}